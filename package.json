{
    "name": "astro-table-of-contents",
    "version": "1.0.1",
    "description": "Toc integration for Astro projects, providing a simple way to generate and display a table of contents for your content. Supports both static and dynamic content.",
    "main": "index",
    "type": "module",
    "scripts": {
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:run": "vitest run",
        "test:watch": "vitest --watch",
        "test:coverage": "vitest run --coverage",
        "test:report": "vitest run --reporter=html",
        "format": "prettier --write .",
        "format:check": "prettier --check ."
    },

    "exports": {
        ".": "./src/index.ts"
    },
    "keywords": ["astro-component", "withastro", "toc", "table-of-contents", "table of contents", "toc integration", "astro toc", "astro table of contents", "auto toc"],
    "repository": {
        "type": "git",
        "url": "https://github.com/elstron/astro-table-of-contents"
    },
    "author": "",
    "license": "MPL-2.0",
    "packageManager": "pnpm@10.12.3",
    "dependencies": {
        "typescript": "^5.9.2"
    },
    "devDependencies": {
        "@vitest/ui": "3.2.4",
        "astro": "^5.12.8",
        "prettier": "3.6.2",
        "vitest": "^3.2.4"
    }
}
